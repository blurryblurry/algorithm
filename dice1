#include <iostream>
#include <stdio.h>
#include <stdlib.h>
#define _CRT_SECURE_NO_WARNINGS
using namespace std;


int main()
{
	int n, sum = 0;
	cin >> n;
	for (int i = 1;i <= 6;i++)
	{
		sum = n - i;
		if (sum >= 1 && sum <= 6)
		{
			for (int c = 1;c <= 6;c++)
			{
				if(c+sum==n && c<=sum) printf("%d %d\n", i, sum);
			
			}
		}
	}
	return 0;
}
