
#include <iostream>
#include <stdio.h>
#include <stdlib.h>
#define _CRT_SECURE_NO_WARNINGS
using namespace std;


int main()
{
	int n,a[1000],b=1000;
	cin >> n;
	for (int i = 1; i <= n;i++)
	{
		cin >> a[i];
		
		if (b > a[i])
		{
			b = a[i];
		}
	}
	for (int i = 1; i <= n;i++)
	{
		if (b == a[i]) printf("%d ",  i);
	}
	
	
}
