
#include <iostream>
#include <stdio.h>
#define _CRT_SECURE_NO_WARNINGS
using namespace std;


int main()
{	
	int a;
	cin >> a;
	for (int b = 0; b < a;b++)
	{
		if (b == 0 || b == a - 1)
		{
			for (int i = 0; i < a; i++)
			{
				printf("*");
			}
			printf("\n");
		}
		else
			for (int c = 0;c < a;c++)
			{
				if (c == 0 )
				{
					printf("*");
				}
				else if (c == a - 1)
				{
					printf("*\n");
				}
				else printf(" ");
			}
		
	}
	
	return 0;
	
}
