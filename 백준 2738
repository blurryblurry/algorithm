#include <iostream>


using namespace std;

int main()
{
   int N,M;
   cin>>N>>M;
   
   int a1[N][M];
   int a2[N][M];
   
   
   for(int i=0;i<N;i++){
       for(int j=0;j<M;j++){
           cin>>a1[i][j];
       }
   }
   
   for(int i=0;i<N;i++){
       for(int j=0;j<M;j++){
           cin>>a2[i][j];
       }
   }
   for(int i=0;i<N;i++){
       for(int j=0;j<M;j++){
           cout<<a1[i][j]+a2[i][j]<<" ";
       }
       cout<<endl;
   }
   
}
