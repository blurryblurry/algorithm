#include <iostream>
#include <stdio.h>
#define _CRT_SECURE_NO_WARNINGS
using namespace std;


int main()
{
	int a;
	cin >> a;
	for (int i = 1; i < a + 1;i++)
	{


		if (i == 1 || i == (a + 1) /2)
		{
			for (int b = 0;b < a;b++)
			{
				printf("*");
			}
			printf("\n");
		}
		else  printf("*\n");


	}
	return 0;

}
