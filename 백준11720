#include <iostream>
#include <stdio.h>
#include <stdlib.h>
#include <vector>
#include <algorithm>
#include <string>
#define _CRT_SECURE_NO_WARNINGS
using namespace std;


int main()
{
	int N,sum=0;
	char a;
	cin >> N;
	for (int i = 0;i < N;i++)
	{
		cin >> a;
		sum += a - 48;
	}

	printf("%d", sum);
}
