#include <iostream>
#include <string>
using namespace std;

int index(int n)
{
	int i = 666;
	int index = 0;
	string target;
	while (1)
	{
		target = to_string(i);
		for (int j = 0; j < target.length() - 2; j++)
		if(target[j]=='6' && target[j+1]=='6'&&target[j+2]=='6')
		{
			index++;
			if (index == n)
				return i;
			break;
		}
		i++;
	}

}
int main()
{
	int N;
	cin >> N;
	cout << index(N);

}
