#include <iostream>
#include <stdio.h>
#include <string>
using namespace std;
#define _CRT_SECURE_NO_WARNINGS


int main()
{
	string s;
	int arr[26]={3,3,3,4,4,4,5,5,5,6,6,6,7,7,7,8,8,8,8,9,9,9,10,10,10,10};
	int count = 0;
	cin >> s;
	for (int i = 0;i<s.length();i++)
	{
		count+=arr[s[i]-65];
	}
	
	printf("%d", count);
	return 0;
}
