#include <iostream>
#include <string>
using namespace std;

int main() {
  string s;
  string alp = "abcdefghijklmnopqrstuvwxyz";
  int arr[26]={0,};
  cin>>s;
  for(int i=0;i<s.size();i++){
    for(int j=0;j<26;j++){
      if(s[i]==alp[j]) arr[j]++;
    }
  }
  for(int i=0;i<26;i++) cout<<arr[i]<<" ";
  
  return 0;
  
}
