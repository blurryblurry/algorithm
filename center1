#include <iostream>
#include <stdio.h>
#define _CRT_SECURE_NO_WARNINGS
using namespace std;





int main()
{	
	int sum_1=0, sum_2=0, n;
	scanf_s("%d", &n);
	for (int i = 1; i < n;i++)
	{
		sum_1 += i;
	}
	for (int i = n + 1;i<=10000 ;i++)
	{
		sum_2 += i;
		if (sum_2 == sum_1) break;
	
	}

	if (sum_2 == sum_1)
		printf("O");
	else printf("X");
	
	

	

	return 0;

}
