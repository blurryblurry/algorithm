#include <iostream>
#include <map>
using namespace std;

map<string,bool>m;
int N,M;
string str;
int res;

int main() {
  cin>>N>>M; 
  for(int i=0;i<N;i++){
    cin>>str;
    m.insert(make_pair(str,true));
  }
  for(int i=0;i<M;i++){
    cin>>str;
    if(m[str]==true)res++;
  }
  cout<<res;
  return 0;
  
}
