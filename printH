#include <iostream>
#include <stdio.h>
#define _CRT_SECURE_NO_WARNINGS
using namespace std;


int main()
{
	int n;
	cin >> n;
	for (int i = 0; i < n;i++)
	{
		if (i == n / 2)
		{
			for (int a = 0;a < n;a++)	printf("*");
			printf("\n");
		}
		
		else
		{
			for (int c = 0; c < n ;c++)
			{
				if (c == 0) printf("*");
				else if (c == n - 1) printf("*\n");
				else printf(" ");
			}
		}
	}

	return 0;

}
