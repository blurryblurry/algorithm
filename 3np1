#include <iostream>
#include <stdio.h>
#define _CRT_SECURE_NO_WARNINGS
using namespace std;


int main()
{	
	int a;
	cin >> a;
	printf("%d ", a);
	while (1)
	{
		
		if (a % 2 == 0)
		{
			printf("%d ", a / 2);
			a = a / 2;
		}
		else if (a == 1) return 0;
		else
		{
			printf("%d ",a*3+1);
			a = a * 3 + 1;
		}
	}
}
