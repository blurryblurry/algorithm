#include <iostream>
#include <string>
using namespace std;

int main() {
  while(1){
    string s;
    bool flag =false;
    cin>>s;
    if(s=="0")break;
    for(int i=0;i<s.size()/2;i++){
      if(s[i]-'0'!=s[s.size()-i-1]-'0'){
        flag = true;
        cout<<"no"<<endl;
        break;
      }   
      
    }
    if(!flag)cout<<"yes"<<endl;
   
  }
  return 0;
  
}
